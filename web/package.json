{
  "name": "confa",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .ts,.js,.vue --fix --max-warnings=0 src",
    "gen": "apollo codegen:generate --target=typescript --outputFlat src/api/schema.ts && curl http://rtc/room/schema > /tmp/schema && quicktype -s schema -o src/api/room/schema.ts -t Message /tmp/schema"
  },
  "dependencies": {
    "vue": "^3.2.40",
    "vue-router": "^4.0.12",
    "@apollo/client": "^3.3.20",
    "dashjs": "^4.5.0",
    "graphql": "^15.5.1",
    "graphql-tag": "^2.12.4",
    "ion-sdk-js": "^1.8.2"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.0.0",
    "typescript": "^4.4.4",
    "vite": "^2.9.9",
    "vue-tsc": "^0.29.8",
    "sass": "^1.45.1",
    "prettier": "^2.5.1",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^8.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-config-prettier": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "apollo": "^2.33.9",
    "quicktype": "15.0.260"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "webpack", 
        "apollo",
        "apollo-codegen-core",
        "apollo-language-server",
        "@apollographql/graphql-language-service-interface",
        "@apollographql/graphql-language-service-parser",
        "@apollographql/graphql-language-service-types",
        "@apollographql/graphql-language-service-utils"
      ]
    }
  }
}